package jspbook.ch13;

public class Book {
	private String title;
	private String auther;
	private int price;
	private String publisher;
	
	public Book() {
		this.title = "";
		this.auther = "";
		this.price = 0;
		this.publisher = "";
	}

	public Book(String title, String auther, int price, String publisher) {
		this.title = title;
		this.auther = auther;
		this.price = price;
		this.publisher = publisher;
	}
	
	
}
